{
  "name": "openmarket-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "nodemonConfig": {
    "ignore": [
      "swagger-output.json"
    ],
    "exec": "npm run prestart && node"
  },
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles",
    "test:silent": "cross-env NODE_ENV=development jest --silent",
    "prestart": "node ./swagger.js",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./src/bin/www-prod.js",
    "local": "cross-env NODE_ENV=local nodemon -r dotenv/config ./src/bin/www.js",
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv/config ./src/bin/www.js"
  },
  "dependencies": {
    "@lenne.tech/multer-gridfs-storage": "^5.0.6",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "greenlock-express": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongodb": "^6.5",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "shortid": "^2.2.16",
    "socket.io": "^4.7.5",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "tracer": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0"
  }
}
